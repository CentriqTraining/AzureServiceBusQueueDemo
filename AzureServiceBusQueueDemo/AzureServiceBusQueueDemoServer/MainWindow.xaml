<Window x:Class="AzureServiceBusQueueDemoServer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AzureServiceBusQueueDemoServer"
        mc:Ignorable="d"
        Title="Messaging Server" Height="448.651" Width="568.326">
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition Height="34" />
                <RowDefinition Height="2*"/>
            </Grid.RowDefinitions>
            <StackPanel HorizontalAlignment="Right" VerticalAlignment="Top" Orientation="Horizontal" >
                <Button Name="cmdStart" Content="Start Server"  Width="90" Click="CmdStart_Click" />
            </StackPanel>
        <ListBox Name="lstBox1" Grid.Row="1" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" ItemsSource="{Binding}" />
        </Grid>
</Window>
